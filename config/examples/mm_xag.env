# XAG-USD example profile.
# Commodity market example with conservative one-level quoting.

MM_ENVIRONMENT=mainnet
MM_MARKET_NAME=XAG-USD

MM_OFFSET_MODE=dynamic
MM_NUM_PRICE_LEVELS=1

MM_SPREAD_MULTIPLIER=1.5
MM_MIN_OFFSET_BPS=3.0
MM_MAX_OFFSET_BPS=50
MM_MIN_SPREAD_BPS=5.0

MM_ORDER_SIZE_MULTIPLIER=2
MM_SIZE_SCALE_PER_LEVEL=1.0
MM_MAX_POSITION_SIZE=10

MM_INVENTORY_SKEW_FACTOR=0.02
MM_REPRICE_TOLERANCE_PERCENT=1.0
MM_MAX_ORDER_AGE_S=15
